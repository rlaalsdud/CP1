
public class CP_1_2_2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int sum = 0;
		sum = sum + 1;
		System.out.println("1부터 1까지의 합은 " + sum + "입니다.");
		sum = sum + 2;
		System.out.println("1부터 2까지의 합은 " + sum + "입니다.");
		sum = sum + 3;
		System.out.println("1부터 3까지의 합은 " + sum + "입니다.");
		sum = sum + 4;
		System.out.println("1부터 4까지의 합은 " + sum + "입니다.");
		sum = sum + 5;
		System.out.println("1부터 5"
				+ "까지의 합은 " + sum + "입니다.");
	}

}
